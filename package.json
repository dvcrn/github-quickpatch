{
  "name": "gpr",
  "version": "1.0.5",
  "description": "minimal github pull request patcher",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "test": "mocha --compilers js:babel-register",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "npm run test -s",
    "coverage": "babel-node node_modules/.bin/istanbul cover node_modules/.bin/_mocha --",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "prepublish": "babel --optional runtime src -d dist",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvcrn/gpr.git"
  },
  "bin": {
    "gpr": "./dist/bin.js"
  },
  "keywords": [
    "node",
    "github",
    "pull request",
    "pr"
  ],
  "author": "David Mohl <me@dave.cx>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvcrn/github-patcher/issues"
  },
  "homepage": "https://github.com/dvcrn/github-patcher#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "coveralls": "*",
    "dependency-check": "*",
    "doctoc": "*",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "isparta": "*",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "mockery": "^1.4.1",
    "rimraf": "*",
    "sinon": "^1.17.3",
    "tap-spec": "*",
    "tape": "*",
    "watch": "*"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "request": "^2.69.0",
    "yesno": "0.0.1"
  }
}
